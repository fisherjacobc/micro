    "content-range": "^2.0.2",
    "dedent": "^0.7.0",
    "escape-string-regexp": "^5.0.0",
    "fastify": "^4.15.0",
    "file-type": "^18.2.1",
    "fluent-ffmpeg": "^2.1.2",
    "graphql": "^16.6.0",
    "handlebars": "^4.7.7",
    "istextorbinary": "^6.0.0",
    "luxon": "^3.3.0",
    "mercurius": "^12.2.0",
    "mime-types": "^2.1.35",
    "ms": "^3.0.0-canary.1",
    "nanoid": "^4.0.2",
    "nodemailer": "^6.9.1",
    "normalize-url": "^8.0.0",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
