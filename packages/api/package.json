  "devDependencies": {
    "@mikro-orm/cli": "^5.6.15",
    "@swc/core": "^1.3.44",
    "@sylo-digital/scripts": "^1.0.12",
    "@types/bcryptjs": "^2.4.2",
    "@types/bytes": "^3.1.1",
    "@types/dedent": "^0.7.0",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/luxon": "^3.2.0",
    "@types/mime-types": "^2.1.1",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.15.11",
